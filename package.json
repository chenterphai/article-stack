{
  "name": "articlstack",
  "module": "index.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/libs/postgresql.ts",
    "migration:generate": "bun run typeorm migration:generate",
    "migration:create": "bun run typeorm migration:create",
    "migration:run": "bun run typeorm migration:run",
    "migration:revert": "bun run typeorm migration:revert",
    "migration:show": "bun run typeorm migration:show"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/ms": "^2.1.0",
    "express-rate-limit": "^7.5.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@elastic/elasticsearch": "^9.0.2",
    "@graphql-tools/load-files": "^7.0.1",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/node": "^22.15.30",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.6.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "pg": "^8.16.0",
    "prettier": "3.5.3",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "type-graphql": "^2.0.0-rc.2",
    "typeorm": "^0.3.24"
  }
}
